[database]
dburi=mysql+pymysql://nodepool:<%= mysql_password %>@localhost/nodepool

<% @jenkins_masters.each do |master| -%>
[jenkins "<%= master['name'] -%>"]
user=<%= master['user'] %>
apikey=<%= master['apikey'] %>
<% if master.has_key?('credentials') -%>
credentials=<%= master['credentials'] %>
<% end -%>
url=<%= master['url'] %>
<% end %>
